[DEFAULT]
postgres_dsn = postgresql+aiopg:///aio_blog

[app:main]
use = egg:aio_blog
postgres_sqlalchemy_dsn = %(postgres_dsn)s

[alembic]
script_location = alembic
file_template = %%(slug)s_%%(rev)s
sqlalchemy.url = %(postgres_dsn)s

[loggers]
keys = root, sqlalchemy, alembic

[handlers]
keys = console

[logger_alembic]
level = DEBUG
handlers =
qualname = alembic

[logger_root]
level = INFO
handlers = console

[logger_sqlalchemy]
level = INFO
handlers =
qualname = sqlalchemy.engine

[formatters]
keys = generic

[formatter_generic]
format = %(asctime)-15s %(levelname)-5.5s [%(name)s] %(message)s

[handler_console]
class = StreamHandler
args = (sys.stderr,)
level = NOTSET
formatter = generic